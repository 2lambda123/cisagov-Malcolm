{
  "name": "total_bytes_AD",
  "description": "A detector based on total bytes summed.",
  "time_field": "@timestamp",
  "indices": [
    "sessions2-*"
  ],
  "feature_attributes": [
    {
      "feature_name": "total_bytes",
      "feature_enabled": true,
      "aggregation_query": {
        "total_bytes": {
          "sum": {
            "field": "totBytes"
          }
        }
      }
    }
  ],
  "filter_query": {
    "bool": {
      "filter": [
        {
          "exists": {
            "field": "totBytes",
            "boost": 1
          }
        }
      ],
      "adjust_pure_negative": true,
      "boost": 1
    }
  },
  "detection_interval": {
    "period": {
      "interval": 10,
      "unit": "Minutes"
    }
  },
  "window_delay": {
    "period": {
      "interval": 1,
      "unit": "Minutes"
    }
  }
}